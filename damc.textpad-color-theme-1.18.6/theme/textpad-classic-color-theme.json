  "tokenColors": [
    {
      "name": "Classes and functions",
      "scope": [
        "entity.name",
        "entity.name.function.clojure",
        "entity.name.section",
        "entity.name.tag", 
        "entity.name.type",
        "storage.type.groovy",
        "storage.type.java",
