        "variable.other.object",
        "support.function",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#0000AA"
      }
    },
    {
      "name":"Keywords",
      "scope": [
        "constant.character.escape",
        "constant.language",
        "constant.other.placeholder",
        "entity.other.attribute-name.html", 
        "keyword.const",
        "keyword.control",
        "keyword.channel",
        "keyword.function",
        "keyword.import",
        "keyword.interface",
        "keyword.other",
        "keyword.other.new",
        "keyword.package",
        "keyword.type", 
        "keyword.map",
        "storage.control",
        "storage.modifier",
        "storage.type",
        "support.class",
        "support.constant",   
        "support.other", 
        "support.type",
        "support.variable",
        "variable.language",
        "variable.name"
      ],
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": ""
      }
    },
    {
      "name": "Disable colors",
      "scope": [
        "constant.numeric", 
        "entity.name.function",
        "entity.name.type.class" ,
        "entity.name.type.module",
        "variable.other",
        "variable.other.object.property",
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "entity.name.function.operator",
        "keyword.operator",
        "keyword.operator.misc.rust",
        "keyword.operator.logical",
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#AD4011"
      }
    },
    {
      "name": "Strings",
      "scope":[
        "entity.name.import.go",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.variable",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "string.quoted",
        "string.regexp"
      ],
      "settings":{
        "foreground": "#008080",
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment.block",
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": ""
      }
    }  
